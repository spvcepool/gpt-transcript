{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Electron",
            "program": "${workspaceFolder}/node_modules/.bin/electron",
            "args": ["${workspaceFolder}/main.js"],
            "outFiles": ["${workspaceFolder}/**/*.js"],
            "sourceMaps": true,
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron"
        }
    ]
}
